{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    "@emotion",
    "@babel/proposal-class-properties",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "<^w^>": "./src"
        }
      }
    ]
  ]
}
