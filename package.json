{
  "name": "tsleuth",
  "version": "1.0.0",
  "description": "A collection of utilities for the transformation and documentation of typescript 5 code",
  "main": "cli/dist/main.js",
  "scripts": {
    "build-cli": "yarn run webpack --config=./cli/webpack.config.js",
    "lint": "yarn run eslint .",
    "lint:fix": "yarn run eslint --fix .",
    "format": "prettier --write ."
  },
  "keywords": [
    "typescript",
    "documentation-generator",
    "cli"
  ],
  "author": "Michael Sohnen",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/express": "^4.17.18",
    "express": "^4.18.2",
    "ts-loader": "^9.4.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "type": "commonjs"
}
