{
  "name": "tsleuth",
  "version": "1.0.0",
  "description": "A collection of utilities for the transformation and documentation of typescript 5 code",
  "main": "cli/dist/main.js",
  "scripts": {
    "build-cli": "yarn run webpack --config=./cli/webpack.config.js",
    "build-client": "yarn run webpack --config=./client/webpack.config.js",
    "build-all": "yarn run build-cli && yarn run build-client",
    "lint": "yarn run eslint .",
    "lint:fix": "yarn run eslint --fix .",
    "format": "prettier --write ."
  },
  "keywords": [
    "typescript",
    "documentation-generator",
    "cli"
  ],
  "author": "Michael Sohnen",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@types/react": "^18.2.27",
    "@types/react-dom": "^18.2.12",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.50.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "html-webpack-plugin": "^5.5.3",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "5.1.0"
  },
  "dependencies": {
    "@types/express": "^4.17.18",
    "@types/open": "^6.2.1",
    "@types/uuid": "^9.0.5",
    "chalk": "^5.3.0",
    "express": "^4.18.2",
    "ignore": "^5.2.4",
    "open": "^9.1.0",
    "styled-components": "^6.0.8",
    "ts-loader": "^9.4.4",
    "uuid": "^9.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "typescript": "5.1.0"
  },
  "type": "commonjs"
}
