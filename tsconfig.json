{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strictNullChecks": true,
      "outDir": "./dist",
      "rootDir": ".",
      "paths":{
        "<^w^>/*": ["./src/*"]
      }
    },
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ],
    
  }
  